<Page @class="page" as |page|>
  <ActionBar @page={{page}} @class="action-bar">
    <NavigationButton @icon="res://menu" @tap={{action toggleMenu}}></NavigationButton>
    <Label @class="action-bar-title" @text="Home"></Label>
  </ActionBar>

  <GridLayout @class="page-content">
    <Label @class="page-icon fa" @text="&#xf015;"></Label>
    <Label @class="page-placeholder" @text="Hello World"></Label>
  </GridLayout>
</Page>